<?xml version="1.0"?>
<?xml-model href="../../src/proto/parsing/protocol.rnc" type="application/relax-ng-compact-syntax"?>
<?oxygen RNGSchema="../../src/proto/parsing/protocol.rnc" type="compact"?>
<protocol xmlns="https://chaste.cs.ox.ac.uk/nss/protocol/0.1#">
    <!-- An altered S1S2 which assumes models get to steady state quickly, for testing. -->
    <import source="S1S2.xml" mergeDefinitions="true">
        <setInput name="steady_state_beats">
            <cn xmlns="http://www.w3.org/1998/Math/MathML">10</cn>
        </setInput>
        <setInput name="timecourse_duration">
            <cn xmlns="http://www.w3.org/1998/Math/MathML">2000</cn>
        </setInput>
    </import>
</protocol>
